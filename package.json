{
  "name": "gavithra-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "preview": "npm run build && npm run start",
    "export": "next build && next export",
    "export:github": "next build && next export && touch out/.nojekyll",
    "clean": "rm -rf .next out",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod",
    "deploy:github": "npm run export:github",
    "deployment-check": "node deployment-check.js",
    "deployment-status": "node deployment-status.js",
    "quality-check": "npm run lint && npm run type-check && npm run build",
    "pre-deploy": "npm run clean && npm run quality-check && npm run deployment-check"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.362.0",
    "next": "^16.1.6",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^10.0.0",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
